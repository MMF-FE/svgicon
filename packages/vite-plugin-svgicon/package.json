{
    "name": "vite-plugin-svgicon",
    "version": "2.0.2",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "license": "MIT",
    "description": "vite plugin for @yzfe/svgicon",
    "keywords": [
        "vite-plugin",
        "vite",
        "svgicon",
        "@yzfe/svgicon"
    ],
    "author": "Allenice Mo <994298628@qq.com>",
    "homepage": "https://github.com/MMF-FE/svgicon",
    "scripts": {
        "dev": "rollup -c --bundleConfigAsCjs -w",
        "build": "rm -rf dist && rollup -c --bundleConfigAsCjs",
        "prepublishOnly": "yarn build"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "peerDependencies": {
        "@yzfe/react-svgicon": ">=1.0.0",
        "@yzfe/vue3-svgicon": ">=1.0.0",
        "vite": ">=2.0.0-beta.1"
    },
    "peerDependenciesMeta": {
        "@yzfe/react-svgicon": {
            "optional": true
        },
        "@yzfe/vue3-svgicon": {
            "optional": true
        }
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.5",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.5",
        "rollup": "^4.0.2",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.5.0"
    },
    "dependencies": {
        "@types/svgo": "^2.6.3",
        "@yzfe/svgicon-gen": "*",
        "minimatch": "^9.0.3"
    }
}
