{
    "name": "@yzfe/svgicon-gen",
    "version": "1.2.2",
    "description": "A generator to create svgicon object",
    "scripts": {
        "dev": "rollup -c -w",
        "build": "rm -rf dist && rollup -c",
        "prepublishOnly": "yarn build"
    },
    "author": "Allenice Mo <994298628@qq.com>",
    "homepage": "https://github.com/MMF-FE/svgicon",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "directories": {
        "lib": "lib",
        "dist": "dist",
        "typings": "typings"
    },
    "files": [
        "lib",
        "dist",
        "typings"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MMF-FE/svgicon.git"
    },
    "bugs": {
        "url": "https://github.com/MMF-FE/svgicon/issues"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^15.0.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@types/chai": "^4.2.12",
        "@types/lodash": "^4.14.159",
        "@types/mocha": "^8.0.3",
        "@types/node": "^14.0.27",
        "chai": "^4.2.0",
        "mocha": "^8.1.3",
        "rollup": "^2.26.3",
        "rollup-plugin-peer-deps-external": "^2.2.3",
        "rollup-plugin-typescript2": "^0.27.2",
        "ts-node": "^9.0.0"
    },
    "dependencies": {
        "@types/svgo": "^2.6.3",
        "lodash": "^4.17.21",
        "svgo": "^2.8.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "gitHead": "8f4f2a4442e072323ecf76585a533342ddafecb5"
