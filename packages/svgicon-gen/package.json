{
    "name": "@yzfe/svgicon-gen",
    "version": "1.3.0-alpha.0",
    "description": "A generator to create svgicon object",
    "scripts": {
        "dev": "rollup -c --bundleConfigAsCjs -w",
        "build": "rm -rf dist && rollup -c --bundleConfigAsCjs",
        "prepublishOnly": "yarn build"
    },
    "author": "Allenice Mo <994298628@qq.com>",
    "homepage": "https://github.com/MMF-FE/svgicon",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "directories": {
        "lib": "lib",
        "dist": "dist",
        "typings": "typings"
    },
    "files": [
        "lib",
        "dist",
        "typings"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MMF-FE/svgicon.git"
    },
    "bugs": {
        "url": "https://github.com/MMF-FE/svgicon/issues"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.5",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/lodash": "^4.14.159",
        "@types/node": "^14.0.27",
        "rollup": "^4.0.2",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@types/svgo": "^2.6.3",
        "lodash": "^4.17.21",
        "svgo": "^2.8.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "gitHead": "8f4f2a4442e072323ecf76585a533342ddafecb5"
}
